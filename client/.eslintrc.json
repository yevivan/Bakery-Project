{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
          "prettier",
         "airbnb"
        
         
       
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {

             "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
