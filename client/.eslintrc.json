{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
          "prettier",
          "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
         "semi": 0,
          "comma-dangle": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "no-use-before-define": 0



    }
}
