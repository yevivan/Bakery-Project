{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
          "prettier",
          "airbnb"
             
         
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
         "semi": 0,
          "comma-dangle": 0,
             "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
             "eslint linebreak-style": ["error", "windows"]
    }
}
